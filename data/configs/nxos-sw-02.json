{
  "openconfig-interfaces:interfaces": {
    "interface": [
      {
        "name": "Ethernet1/1",
        "config": {
          "name": "Ethernet1/1",
          "description": "Uplink to core-sw-01",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "10.0.3.1",
                      "config": {
                        "ip": "10.0.3.1",
                        "prefix-length": 30
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "Ethernet1/2",
        "config": {
          "name": "Ethernet1/2",
          "description": "Uplink to core-sw-02",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "10.0.4.1",
                      "config": {
                        "ip": "10.0.4.1",
                        "prefix-length": 30
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "Ethernet1/10",
        "config": {
          "name": "Ethernet1/10",
          "description": "Connection to nxos-sw-01",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "openconfig-vlan:switched-vlan": {
          "config": {
            "interface-mode": "TRUNK",
            "trunk-vlans": [10, 20, 30, 40]
          }
        }
      },
      {
        "name": "port-channel100",
        "config": {
          "name": "port-channel100",
          "description": "VPC to downstream devices",
          "type": "iana-if-type:ieee8023adLag",
          "enabled": true
        },
        "openconfig-if-aggregate:aggregation": {
          "config": {
            "lag-type": "LACP"
          }
        },
        "openconfig-vlan:switched-vlan": {
          "config": {
            "interface-mode": "TRUNK",
            "trunk-vlans": [10, 20, 30, 40]
          }
        }
      },
      {
        "name": "Vlan10",
        "config": {
          "name": "Vlan10",
          "description": "Management SVI",
          "type": "iana-if-type:l3ipvlan",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "192.168.10.32",
                      "config": {
                        "ip": "192.168.10.32",
                        "prefix-length": 24
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "Vlan40",
        "config": {
          "name": "Vlan40",
          "description": "Guest Network SVI",
          "type": "iana-if-type:l3ipvlan",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "192.168.40.1",
                      "config": {
                        "ip": "192.168.40.1",
                        "prefix-length": 24
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "Loopback0",
        "config": {
          "name": "Loopback0",
          "description": "BGP Router ID",
          "type": "iana-if-type:softwareLoopback",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "192.168.255.32",
                      "config": {
                        "ip": "192.168.255.32",
                        "prefix-length": 32
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      }
    ]
  },
  "openconfig-vlan:vlans": {
    "vlan": [
      {
        "vlan-id": 10,
        "config": {
          "vlan-id": 10,
          "name": "Management",
          "status": "ACTIVE"
        }
      },
      {
        "vlan-id": 20,
        "config": {
          "vlan-id": 20,
          "name": "Production",
          "status": "ACTIVE"
        }
      },
      {
        "vlan-id": 30,
        "config": {
          "vlan-id": 30,
          "name": "Development",
          "status": "ACTIVE"
        }
      },
      {
        "vlan-id": 40,
        "config": {
          "vlan-id": 40,
          "name": "Guest",
          "status": "ACTIVE"
        }
      }
    ]
  },
  "openconfig-network-instance:network-instances": {
    "network-instance": [
      {
        "name": "default",
        "config": {
          "name": "default",
          "type": "openconfig-network-instance-types:DEFAULT_INSTANCE"
        },
        "protocols": {
          "protocol": [
            {
              "identifier": "openconfig-policy-types:BGP",
              "name": "BGP",
              "bgp": {
                "global": {
                  "config": {
                    "as": 65001,
                    "router-id": "192.168.255.32"
                  }
                },
                "neighbors": {
                  "neighbor": [
                    {
                      "neighbor-address": "10.0.3.2",
                      "config": {
                        "neighbor-address": "10.0.3.2",
                        "peer-as": 65001,
                        "description": "iBGP to core-sw-01"
                      }
                    },
                    {
                      "neighbor-address": "10.0.4.2",
                      "config": {
                        "neighbor-address": "10.0.4.2",
                        "peer-as": 65001,
                        "description": "iBGP to core-sw-02"
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      }
    ]
  },
  "openconfig-system:system": {
    "config": {
      "hostname": "nxos-sw-02",
      "domain-name": "example.com"
    },
    "clock": {
      "config": {
        "timezone-name": "UTC"
      }
    },
    "ntp": {
      "config": {
        "enabled": true
      },
      "servers": {
        "server": [
          {
            "address": "192.168.1.100",
            "config": {
              "address": "192.168.1.100",
              "version": 4,
              "prefer": true
            }
          }
        ]
      }
    }
  }
