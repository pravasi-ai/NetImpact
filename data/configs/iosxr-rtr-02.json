{
  "openconfig-interfaces:interfaces": {
    "interface": [
      {
        "name": "GigabitEthernet0/0/0/0",
        "config": {
          "name": "GigabitEthernet0/0/0/0",
          "description": "Connection to iosxr-rtr-01",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "10.0.7.2",
                      "config": {
                        "ip": "10.0.7.2",
                        "prefix-length": 30
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "GigabitEthernet0/0/0/1",
        "config": {
          "name": "GigabitEthernet0/0/0/1",
          "description": "WAN Connection to ISP",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "203.0.113.2",
                      "config": {
                        "ip": "203.0.113.2",
                        "prefix-length": 30
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "GigabitEthernet0/0/0/2",
        "config": {
          "name": "GigabitEthernet0/0/0/2",
          "description": "Backup WAN Connection",
          "type": "iana-if-type:ethernetCsmacd",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "198.51.100.2",
                      "config": {
                        "ip": "198.51.100.2",
                        "prefix-length": 30
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      },
      {
        "name": "Loopback0",
        "config": {
          "name": "Loopback0",
          "description": "BGP Router ID",
          "type": "iana-if-type:softwareLoopback",
          "enabled": true
        },
        "subinterfaces": {
          "subinterface": [
            {
              "index": 0,
              "openconfig-if-ip:ipv4": {
                "addresses": {
                  "address": [
                    {
                      "ip": "192.168.255.42",
                      "config": {
                        "ip": "192.168.255.42",
                        "prefix-length": 32
                      }
                    }
                  ]
                }
              }
            }
          ]
        }
      }
    ]
  },
  "openconfig-network-instance:network-instances": {
    "network-instance": [
      {
        "name": "default",
        "config": {
          "name": "default",
          "type": "openconfig-network-instance-types:DEFAULT_INSTANCE"
        },
        "protocols": {
          "protocol": [
            {
              "identifier": "openconfig-policy-types:BGP",
              "name": "BGP",
              "bgp": {
                "global": {
                  "config": {
                    "as": 65002,
                    "router-id": "192.168.255.42"
                  }
                },
                "neighbors": {
                  "neighbor": [
                    {
                      "neighbor-address": "10.0.7.1",
                      "config": {
                        "neighbor-address": "10.0.7.1",
                        "peer-as": 65001,
                        "description": "eBGP to iosxr-rtr-01"
                      }
                    },
                    {
                      "neighbor-address": "203.0.113.1",
                      "config": {
                        "neighbor-address": "203.0.113.1",
                        "peer-as": 65000,
                        "description": "eBGP to ISP Primary"
                      }
                    },
                    {
                      "neighbor-address": "198.51.100.1",
                      "config": {
                        "neighbor-address": "198.51.100.1",
                        "peer-as": 65000,
                        "description": "eBGP to ISP Backup"
                      }
                    }
                  ]
                }
              }
            },
            {
              "identifier": "openconfig-policy-types:STATIC",
              "name": "static",
              "static-routes": {
                "static": [
                  {
                    "prefix": "0.0.0.0/0",
                    "config": {
                      "prefix": "0.0.0.0/0"
                    },
                    "next-hops": {
                      "next-hop": [
                        {
                          "index": "1",
                          "config": {
                            "index": "1",
                            "next-hop": "203.0.113.1",
                            "metric": 100
                          }
                        },
                        {
                          "index": "2",
                          "config": {
                            "index": "2",
                            "next-hop": "198.51.100.1",
                            "metric": 200
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
  "openconfig-acl:acl": {
    "acl-sets": {
      "acl-set": [
        {
          "name": "WAN_INBOUND",
          "type": "ACL_IPV4",
          "config": {
            "name": "WAN_INBOUND",
            "type": "ACL_IPV4"
          },
          "acl-entries": {
            "acl-entry": [
              {
                "sequence-id": 10,
                "ipv4": {
                  "config": {
                    "source-address": "0.0.0.0/0",
                    "destination-address": "192.168.0.0/16",
                    "protocol": "openconfig-packet-match-types:TCP"
                  }
                },
                "transport": {
                  "config": {
                    "destination-port": 443
                  }
                },
                "actions": {
                  "config": {
                    "forwarding-action": "openconfig-acl:ACCEPT"
                  }
                }
              },
              {
                "sequence-id": 20,
                "ipv4": {
                  "config": {
                    "source-address": "0.0.0.0/0",
                    "destination-address": "192.168.0.0/16",
                    "protocol": "openconfig-packet-match-types:TCP"
                  }
                },
                "transport": {
                  "config": {
                    "destination-port": 80
                  }
                },
                "actions": {
                  "config": {
                    "forwarding-action": "openconfig-acl:ACCEPT"
                  }
                }
              },
              {
                "sequence-id": 100,
                "ipv4": {
                  "config": {
                    "source-address": "0.0.0.0/0",
                    "destination-address": "0.0.0.0/0",
                    "protocol": "openconfig-packet-match-types:IP"
                  }
                },
                "actions": {
                  "config": {
                    "forwarding-action": "openconfig-acl:DROP"
                  }
                }
              }
            ]
          }
        }
      ]
    }
  },
  "openconfig-system:system": {
    "config": {
      "hostname": "iosxr-rtr-02",
      "domain-name": "example.com"
    },
    "clock": {
      "config": {
        "timezone-name": "UTC"
      }
    },
    "ntp": {
      "config": {
        "enabled": true
      },
      "servers": {
        "server": [
          {
            "address": "192.168.1.100",
            "config": {
              "address": "192.168.1.100",
              "version": 4,
              "prefer": true
            }
          }
        ]
      }
    }
  }
