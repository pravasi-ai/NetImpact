{
  "cisco-ios": {
    "preferred_format": "openconfig",
    "description": "Cisco IOS devices prefer OpenConfig format for standardized configuration across vendors",
    "structure_examples": {
      "routing": {
        "bgp": {
          "global": {
            "config": {
              "as": 65001,
              "router-id": "10.0.0.1"
            }
          },
          "neighbors": []
        },
        "ospf": {
          "global": {
            "config": {
              "router-id": "10.0.0.1"
            }
          },
          "areas": {
            "area": [
              {
                "identifier": "0.0.0.0",
                "config": {
                  "identifier": "0.0.0.0"
                }
              }
            ]
          }
        }
      },
      "openconfig-interfaces:interfaces": {
        "interface": [
          {
            "name": "GigabitEthernet0/0/1",
            "config": {
              "name": "GigabitEthernet0/0/1",
              "description": "Connection to core"
            }
          }
        ]
      },
      "openconfig-acl:acl": {
        "acl-sets": {
          "acl-set": [
            {
              "name": "PERMIT_MANAGEMENT",
              "type": "ACL_IPV4",
              "config": {
                "name": "PERMIT_MANAGEMENT",
                "type": "ACL_IPV4"
              }
            }
          ]
        }
      }
    }
  },
